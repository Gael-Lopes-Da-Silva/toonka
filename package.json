{
  "name": "my-qwik-empty-starter",
  "description": "Blank project with routing included",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npx qwik build",
    "build.client": "npx vite build",
    "build.preview": "npx vite build --ssr src/entry.preview.tsx",
    "build.types": "npx tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "npx vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "drizzle:check": "npx drizzle-kit check",
    "drizzle:generate": "npx drizzle-kit generate",
    "drizzle:migrate": "npx drizzle-kit migrate",
    "drizzle:pull": "npx drizzle-kit pull",
    "drizzle:push": "npx drizzle-kit push",
    "drizzle:studio": "npx drizzle-kit studio",
    "drizzle:up": "npx drizzle-kit up",
    "fmt": "npx prettier --write .",
    "fmt.check": "npx prettier --check .",
    "lint": "npx eslint \"src/**/*.ts*\"",
    "postinstall": "npx drizzle-kit generate",
    "preview": "npx qwik build preview && vite preview --open",
    "start": "npx vite --mode ssr --host",
    "qwik": "npx qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.15",
    "@builder.io/qwik-city": "^1.2.15",
    "@eslint/js": "latest",
    "@tailwindcss/vite": "^4.0.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "20.14.11",
    "better-sqlite3": "^11.8.1",
    "drizzle-kit": "^0.18.1",
    "drizzle-orm": "^0.39.0",
    "eslint": "9.25.1",
    "eslint-plugin-qwik": "^1.14.1",
    "globals": "16.0.0",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.0",
    "typescript": "5.4.5",
    "typescript-eslint": "8.26.1",
    "undici": "*",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^4.2.1"
  }
}
